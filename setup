#!/bin/sh
echo "removing default dotfiles..."
echo rm $HOME/.vimrc
echo rm $HOME/.zshrc
echo rm $HOME/.tmux.conf
echo "setting up symlinks..."
echo ln -s $HOME/.dotfiles/vim/vimrc $HOME/.vimrc
echo ln -s $HOME/.dotfiles/zsh/zshrc $HOME/.zshrc
echo ln -s $HOME/.dotfiles/tmux/tmux.conf $HOME/.tmux.conf
